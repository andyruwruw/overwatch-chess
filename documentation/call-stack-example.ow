variables
{
	global:
		0: Stack
}

rule("Initialize")
{
	event
	{
		Ongoing - Global;
	}

	actions
	{
		// Inicializing Call Stack
		Global.Stack = Empty Array;

    // Frame Pointer
    Global.Stack[0] = 2;
    // Stack Pointer
    Global.Stack[1] = 2;



		// Adding variable1
		Global.Stack[Global.StackPointer] = 2;
		Global.StackLabels[Global.StackPointer] = Custom String("variable1");
		Global.StackPointer += 1;

		// Calling subroutine
		Call Subroutine(Sub0);
	}
}